{
	"name": "66bit",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint:dev": "eslint \"src/**/*.ts\"",
		"lint": "lint:dev --quiet",
		"format": "prettier --write \"src/**/*\"",
		"prepare": "cd .. && husky install frontend/.husky",
