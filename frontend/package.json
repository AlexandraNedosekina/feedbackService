		"@mantine/hooks": "^5.7.2",
		"@mantine/next": "^5.7.2",
		"@mantine/notifications": "^5.7.2",
		"@tabler/icons": "^1.111.0",
		"@tanstack/react-query": "^4.16.1",
		"@xstate/react": "^3.0.1",
		"axios": "^1.1.3",
		"cookies-next": "^2.1.1",
		"dayjs": "^1.11.6",
		"immer": "^9.0.16",
		"material-symbols": "^0.2.15",
		"next": "^13.0.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
